start:
    leay helloworld,pcr
    jsr print
    rts

const textscreenbase=$400

print:
    pshs a,x,y
    ldx #textscreenbase
printloop:
    lda ,y+
    beq printover
    cmpa #$40
    bhs print6847
    adda #$40
print6847:
    sta ,x+
    bra printloop
printover:
    puls a,x,y
    rts


helloworld:
    fcb "HELLO WORLD",0

